<?xml version="1.0" encoding="utf-8"?>

<odoo> 
    <data noupdate="1">
        <record id="rfq_approval" model="ir.cron">
            <field name="name">rfq_approver</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall">1</field>
            <field name="nextcall" eval="(datetime.now() + timedelta(days=1)).strftime('%Y-%m-%d 12:00:00')"/>
            <field name="model_id" ref="model_scheduled_approval_for_rfq"/>
            <field name="code">model._approve_rfqs()</field>
<!--             <field name="args" eval="" /> -->
            <!--<field name="priority" eval="5" />-->
        </record>
    </data>
</odoo>